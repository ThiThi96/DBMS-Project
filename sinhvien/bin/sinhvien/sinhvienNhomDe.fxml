<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sinhvien.sinhvienNhomDeController">
   <children>
      <Label fx:id="maLop" layoutX="300.0" layoutY="65.0" />
      <Label fx:id="tenLop" layoutX="400" layoutY="65.0" />
   </children>
   <children>
      <TabPane fx:id="tabPane" prefHeight="600.0" prefWidth="1000.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab closable="false" text="Đề">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  	 <children>
                        <TableView fx:id="tableDeConHanDK" layoutX="169.0" layoutY="87.0" prefHeight="398.0" prefWidth="662.0">
                          <columns>
                            <TableColumn prefWidth="75.0" text="C1" />
                            <TableColumn prefWidth="75.0" text="C2" />
                          </columns>
                        </TableView>
                        <Label fx:id="lblDSDeConHanDK" layoutX="313.0" layoutY="48.0" text="DANH SÁCH ĐỀ CÒN HẠN ĐĂNG KÝ">
                           <font>
                              <Font size="25.0" />
                           </font>
                        </Label>
                        <Button fx:id="btnDangKyDe" layoutX="550.0" layoutY="504.0" mnemonicParsing="false" onMouseClicked="#dangKyDe_CLicked" text="Đăng ký " textAlignment="CENTER">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnQuayVeTC" layoutX="698.0" layoutY="504.0" mnemonicParsing="false" onMouseClicked="#quaylaiClicked" text="Quay lại">
                           <font>
                              <Font size="15.0" />
                           </font>
                        </Button>
                        <Button fx:id="btnXemDSDeDaDK" layoutX="23.0" layoutY="21.0" mnemonicParsing="false" onMouseClicked="#xemDSDeDaDK_Clicked" text="Xem đề đã đăng ký">
                           <font>
                              <Font size="16.0" />
                           </font>
                        </Button>
                     </children>
              </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="tabNhom" closable="false" text="Nhóm">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                 <children>
                    <TableView fx:id="tableNhom" editable="false" layoutX="29.0" layoutY="57.0" prefHeight="400.0" prefWidth="900.0" AnchorPane.bottomAnchor="130.0" AnchorPane.leftAnchor="100.0" AnchorPane.rightAnchor="100.0" AnchorPane.topAnchor="80.0">
                      
                    </TableView>
                        <Button fx:id="btnQuayve" layoutX="747.0" layoutY="475.0" mnemonicParsing="false" onAction="#quaylaiClicked" prefHeight="25.0" prefWidth="153.0" text="Quay về" AnchorPane.leftAnchor="747.0" AnchorPane.rightAnchor="100.0" />
                        <Button fx:id="btnDangkynhom" layoutX="225.0" layoutY="475.0" mnemonicParsing="false" onAction="#dangkynhomClicked" prefHeight="25.0" prefWidth="154.0" text="Đăng ký nhóm" />
                        <Button fx:id="btnRutnhom" disable="true" layoutX="571.0" layoutY="475.0" mnemonicParsing="false" onAction="#rutnhomClicked" prefHeight="25.0" prefWidth="152.0" text="Rút nhóm" AnchorPane.leftAnchor="571.0" AnchorPane.rightAnchor="277.0" />
                        <Button fx:id="btnChitietnhom" disable="true" layoutX="400.0" layoutY="475.0" mnemonicParsing="false" onAction="#chitietnhomClicked" prefHeight="25.0" prefWidth="150.0" text="Chi tiết nhóm" />
                 </children>
               </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
      <ImageView fx:id="iconUser" fitHeight="29.0" fitWidth="45.0" layoutX="870.0" layoutY="8.0" pickOnBounds="true" preserveRatio="true">
      	<image>
             <Image url="@user.png" />
         </image>
      </ImageView>
      <Label fx:id="tendn" layoutX="670.0" layoutY="14.0" prefHeight="17.0" prefWidth="191.0" />
      <Label fx:id="dangxuat" layoutX="920.0" layoutY="14.0" text="Đăng xuất" />
   </children>
</AnchorPane>
