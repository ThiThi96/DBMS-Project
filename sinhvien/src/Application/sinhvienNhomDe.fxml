             <Image url="@user.png" />
         </image>
      </ImageView>
      <Label fx:id="tendn" layoutX="670.0" layoutY="14.0" prefHeight="17.0" prefWidth="191.0" />
      <Label fx:id="dangxuat" layoutX="920.0" layoutY="14.0" text="Đăng xuất" />
