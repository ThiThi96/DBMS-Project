                        <TableView fx:id="tableDeConHanDK" layoutX="169.0" layoutY="140.0" prefHeight="345.0" prefWidth="662.0" AnchorPane.leftAnchor="160.0" AnchorPane.rightAnchor="160.0" />
                        <Label fx:id="lblDSDeConHanDK" layoutX="160.0" layoutY="92.0" text="DANH SÁCH ĐỀ CÒN HẠN ĐĂNG KÝ">
                           <font>
                              <Font name="Times New Roman Bold" size="18.0" />
                           </font>
                        </Label>
                        <Button fx:id="btnDangKyDe" layoutX="525.0" layoutY="504.0" mnemonicParsing="false" onMouseClicked="#dangKyDe_CLicked" prefHeight="31.0" prefWidth="134.0" text="Đăng ký " textAlignment="CENTER">
